@if (this.TempData["Notification"] != null)
{
    <div class="alert alert-dismissible alert-success">
        <button type="button" class="close" data-dismiss="alert">&#10006;</button>
        @TempData["Notification"].ToString()
    </div>
}
@if (this.TempData["NotificationError"] != null)
{
    <div class="alert alert-dismissible alert-danger">
        <button type="button" class="close" data-dismiss="alert">&#10006;</button>
        @TempData["NotificationError"].ToString()
    </div>
}
